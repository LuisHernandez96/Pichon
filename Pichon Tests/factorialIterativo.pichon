FUNCTIONS{
    function int factorialIterativo(int n){
        int ret = 1;
        int i;
        for(i = n; i > 0; i = i-1){
            ret = ret * i;
        };

        return ret;
    }
}


ENVIRONMENT{
    int a;

    for(a = 1; a <= 5; a = a + 1){
        print(factorialIterativo(a));
    };
}

